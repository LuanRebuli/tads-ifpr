    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="v1" type="Integer" array="True" size="10"/>
            <declare name="v2" type="Integer" array="True" size="10"/>
            <declare name="V" type="Integer" array="True" size="20"/>
            <declare name="i, b" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="9" direction="inc" step="1">
                <assign variable="v1[i]" expression="random(50)"/>
                <assign variable="v2[i]" expression="random(50)"/>
            </for>
            <output expression="&quot; Vetor A: &quot;" newline="False"/>
            <for variable="i" start="0" end="9" direction="inc" step="1">
                <output expression="v1[i] &amp; &quot; &quot;" newline="False"/>
            </for>
            <output expression="&quot; &quot;" newline="True"/>
            <output expression="&quot; Vetor B: &quot;" newline="False"/>
