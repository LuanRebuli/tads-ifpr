                <output expression="v2[i] &amp; &quot; &quot;" newline="False"/>
            </for>
            <output expression="&quot; &quot;" newline="True"/>
            <for variable="i" start="0" end="9" direction="inc" step="1">
                <assign variable="V[2 * i]" expression="v1[i]"/>
                <assign variable="V[2 * i + 1]" expression="v2[i]"/>
            </for>
            <output expression="&quot;Vetor V: &quot;" newline="False"/>
            <for variable="i" start="0" end="19" direction="inc" step="1">
